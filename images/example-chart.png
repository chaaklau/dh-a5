<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 600 400" width="600" height="400">
  <!-- Background -->
  <rect width="600" height="400" fill="#f8f9fa"/>
  
  <!-- Title -->
  <text x="300" y="30" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#2c3e50">
    Example Data Visualization
  </text>
  
  <!-- Bars -->
  <rect x="80" y="250" width="60" height="140" fill="#3498db" opacity="0.8"/>
  <rect x="160" y="180" width="60" height="210" fill="#2ecc71" opacity="0.8"/>
  <rect x="240" y="150" width="60" height="240" fill="#9b59b6" opacity="0.8"/>
  <rect x="320" y="200" width="60" height="190" fill="#f1c40f" opacity="0.8"/>
  <rect x="400" y="220" width="60" height="170" fill="#e74c3c" opacity="0.8"/>
  
  <!-- Labels -->
  <text x="110" y="410" text-anchor="middle" font-family="Arial" font-size="12" fill="#2c3e50">Cat A</text>
  <text x="190" y="410" text-anchor="middle" font-family="Arial" font-size="12" fill="#2c3e50">Cat B</text>
  <text x="270" y="410" text-anchor="middle" font-family="Arial" font-size="12" fill="#2c3e50">Cat C</text>
  <text x="350" y="410" text-anchor="middle" font-family="Arial" font-size="12" fill="#2c3e50">Cat D</text>
  <text x="430" y="410" text-anchor="middle" font-family="Arial" font-size="12" fill="#2c3e50">Cat E</text>
  
  <!-- Values -->
  <text x="110" y="240" text-anchor="middle" font-family="Arial" font-size="14" font-weight="bold" fill="#2c3e50">45</text>
  <text x="190" y="170" text-anchor="middle" font-family="Arial" font-size="14" font-weight="bold" fill="#2c3e50">72</text>
  <text x="270" y="140" text-anchor="middle" font-family="Arial" font-size="14" font-weight="bold" fill="#2c3e50">88</text>
  <text x="350" y="190" text-anchor="middle" font-family="Arial" font-size="14" font-weight="bold" fill="#2c3e50">61</text>
  <text x="430" y="210" text-anchor="middle" font-family="Arial" font-size="14" font-weight="bold" fill="#2c3e50">53</text>
  
  <!-- Axis -->
  <line x1="60" y1="390" x2="480" y2="390" stroke="#2c3e50" stroke-width="2"/>
  <line x1="60" y1="100" x2="60" y2="390" stroke="#2c3e50" stroke-width="2"/>
  
  <!-- Y-axis label -->
  <text x="20" y="250" text-anchor="middle" font-family="Arial" font-size="12" fill="#2c3e50" transform="rotate(-90, 20, 250)">
    Frequency
  </text>
  
  <!-- Caption -->
  <text x="300" y="435" text-anchor="middle" font-family="Arial" font-size="11" fill="#7f8c8d" font-style="italic">
    Figure: Sample bar chart - replace with your actual data visualization
  </text>
</svg>
